{
    "src_folders": [
        "test/e2e"
    ],
    "output_folder": "test/.build/reports",
    "custom_commands_path": "",
    "custom_assertions_path": "",
    "page_objects_path": "",
    "globals_path": "",
    "disable_colors": false,
    "selenium": {
        "start_process": true,
        "server_path": "",
        "log_path": "",
        "host": "127.0.0.1",
        "port": 4444,
        "cli_args": {
            "webdriver.chrome.driver": "",
            "webdriver.ie.driver": ""
        }
    },
    "parallel_process_delay": 10,
    "test_workers": {
        "enabled": false,
        "workers": "auto"
    },
    "test_settings": {
        "default": {
            "launch_url": "http://localhost",
            "selenium_port": 4444,
            "selenium_host": "localhost",
            "silent": true,
            "screenshots": {
                "enabled": true,
                "on_failure": true,
                "on_error": false,
                "path": "test/.build/screenshots"
            },
            "desiredCapabilities": {
                "browserName": "phantomjs",
                "javascriptEnabled": true,
                "acceptSslCerts": true,
                "phantomjs.binary.path": ""
            }
        }
    }
